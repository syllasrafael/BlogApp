
<div class="mt-5">
    <h1>{{categoria.data}}</h1>
</div>
{{#each postagens}}

<div class="card mt-3">
    <div class="card-body mt-3">
    
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>
        <a href="/postagem/{{slug}}"><button class="btn btn-outline-success btn-sm">Leia mais</button></a>
        <hr>
        <small>Data de publicação: {{data}}</small>
    
    </div>
</div>
{{else}}
<h5>Nenhuma postagem</h5>
{{/each}}